{
  "compileOnSave": false,
  "compilerOptions": {
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ],
    "experimentalDecorators": true,
    "jsx": "react",
    "target": "es2017",
    "module": "es2015",
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "allowJs": true
  }
}
